MDScreen:
    name: "rest"
    MDFloatLayout:

        MDIconButton:
            icon: "arrow-left"
            pos_hint: {"center_y": .95}
            user_font_size: "30sp"
            theme_text_color: "Custom"
            text_color: 0.1, 0.09, 0.22, 1  
            on_release:
                root.manager.transition.direction = "right"
                root.manager.current = "login"
        MDLabel:
            text: "Rest Password"
            bold: True
            font_style: "H6"
            font_size: "21sp"
            halign: "center"
            pos_hint: {"center_x": 0.5, "center_y": 0.93}
            theme_text_color: "Custom"
            text_color: rgba(6, 1, 76) 